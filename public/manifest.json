  "shortcuts": [
    {
      "name": "Interactive Map",
      "short_name": "Map",
      "description": "View Pokemon locations on the interactive map",
      "url": "/mapa",
      "icons": [
        {
          "src": "/icons/map-shortcut.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Wiki Hub",
      "short_name": "Wiki",
      "description": "Access Pokemon guides and documentation",
      "url": "/wiki",
      "icons": [
        {
          "src": "/icons/wiki-shortcut.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Download Game",
      "short_name": "Download",
      "description": "Download the PokeArkus game",
      "url": "/download",
      "icons": [
        {
          "src": "/icons/download-shortcut.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", ".png", ".jpg", ".jpeg", ".gif"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "image/*": [".png", ".jpg", ".jpeg", ".gif"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "pokearkus",
      "url": "/protocol?url=%s"
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "edge_side_panel": {
    "preferred_width": 400
  }
}